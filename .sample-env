# rename this file to .env, replace variables
# MySQL Database configuration
DB_HOST=<hostname>
DB_PORT=3306
DB_USER=<username>
DB_PASSWORD=<password>
DB_DATABASE=<schema>
DB_CONNECT_TIMEOUT=10000


INIT_EMAIL=<email>

# Auth0 variables
AUTH0_SECRET=<secret>
AUTH0_BASE_URL=<url>
AUTH0_CLIENT_ID=<id>
AUTH0_ISSUER_BASE_URL=<url>